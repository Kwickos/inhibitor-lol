{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "regions": ["cdg1"],
  "functions": {
    "src/app/api/refresh-matches/[puuid]/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/analysis/[puuid]/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/scrape-high-elo/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate=300"
        }
      ]
    }
  ]
}
